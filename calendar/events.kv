<Event@RelativeLayout>:
    canvas.before:
        Color:
            rgba: [0.21568627450980393, 0.6705882352941176, 0.7843137254901961, 0.5]
        Line:
            rectangle: 0, 0, self.width, self.height
    Label:
        text: "Event"


<EventWeekend@RelativeLayout>:
    canvas.before:
        Color:
            rgba: 0.1, 0.27, 0.27, 1
        Rectangle:
            pos: 0,0
            size: self.width * 1, self.height * 1

        Color:
            rgba: [0.21568627450980393, 0.6705882352941176, 0.7843137254901961, 0.5]
        Line:
            rectangle: 0, 0, self.width, self.height
    Label:
        text: "Event"


