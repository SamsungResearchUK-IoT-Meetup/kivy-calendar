<CalendarStatus@BoxLayout>:
    orientation: "horizontal"
	cal_year: year
	cal_month: month
    Button:
        text: "Previous Month"

	Label:
		id: month
		markup: True
		text: '[size=24px][color=#37abc8ff]Month[/color]'
	Lable:
	    id: year
	    markup: True
	    text: '[size=18px][color=#37abc8ff][i]Year[/i][/color]'
    Button:
        text: "Next Month"
